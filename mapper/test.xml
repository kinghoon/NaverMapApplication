<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.smhrd.mapper.test">

<select id="selert" parameterType="com.smhrd.entity.Test">
	SELECT * FROM TEST
</select>

<select id="login" parameterType="com.smhrd.entity.Test" resultType="com.smhrd.entity.Test">
	SELECT * FORM TEST WHERE ID = {id} AND PW = #{pw}
</select>
</mapper>

